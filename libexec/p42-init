#!/usr/bin/env bash

# if [ -e ./p42.yaml ]; then
#   echo p42: directory already contains a p42.yaml file
#   exit 1
# fi

include aws

read -d '' defaults <<EOF
name: $(basename $(pwd))
registry: $(get_registry_domain)
EOF

_interview="${share}/interviews/init.yaml"
interview=$(yaml template <(echo "${defaults}") ${_interview})
p42 interview <(echo "${interview}") > ./p42.yaml
